{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "no-underscore-dangle": "off",
    "react/sort-comp": "off",
    "import/prefer-default-export": "off",
    "react/forbid-prop-types": "warn"
  },
  "plugins": ["babel", "react", "import", "jsx-a11y"]
}
